{
  "type": "module",
  "name": "oasprey",
  "version": "1.0.0",
  "description": "Jest matchers for testing OpenAPI specifications",
  "keywords": ["jest", "openapi", "testing", "api", "validation"],
  "homepage": "https://github.com/grove-platform/OASprey#readme",
  "bugs": {
    "url": "https://github.com/grove-platform/OASprey/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/grove-platform/OASprey.git"
  },
  "license": "MIT",
  "author": "Grove Platform",
  "main": "dist/index.js",
  "files": [
    "dist/",
    "README.md",
    "LICENSE"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "clean": "rimraf dist coverage",
    "format": "prettier --write .",
    "lint": "eslint 'src/**/*.{js,ts}' 'tests/**/*.{js,ts}'",
    "build": "tsc",
    "test": "jest",
    "test:ci": "jest --ci --runInBand",
    "prepare": "husky",
    "prepublishOnly": "npm run lint && npm run typecheck:all && npm run test && npm run build",
    "typecheck:tests": "tsc --project tsconfig.test.json",
    "typecheck:common": "tsc --project tsconfig.commonTestResources.json",
    "typecheck:all": "npm run typecheck:tests && npm run typecheck:common",
    "coverage": "jest --coverage --coverageThreshold='{\"global\": {\"branches\": 80, \"functions\": 80, \"lines\": 80, \"statements\": 80}}'"
  },
  "devDependencies": {
    "@types/express": "^5.0.3",
    "@types/fs-extra": "^11.0.4",
    "@types/jest": "^30.0.0",
    "@types/js-yaml": "^4.0.9",
    "@typescript-eslint/eslint-plugin": "8.40.0",
    "@typescript-eslint/parser": "8.40.0",
    "eslint": "9.33.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jest": "^29.0.1",
    "express": "^5.1.0",
    "husky": "^9.1.7",
    "jest": "^30.0.5",
    "prettier": "^3.6.2",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.4.1",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "axios": "^1.12.2",
    "cartesian-product": "^2.1.2",
    "fs-extra": "11.3.1",
    "jest-matcher-utils": "^30.0.5",
    "js-yaml": "^4.1.1",
    "openapi-response-validator": "^12.1.3",
    "openapi-schema-validator": "^12.1.3",
    "openapi-types": "^12.1.3",
    "path-to-regexp": "^8.2.0"
  }
}
